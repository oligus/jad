{
  "name": "oligus/jad",
  "description": "Json Api to Doctrine ORM",
  "keywords": [
    "json",
    "api",
    "doctrine",
    "orm"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "oligus",
      "email": "oli.gustafsson@gmail.com"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-clover=coverage.xml",
    "lint": "phpcs src",
    "lint-fix": "phpcbf src",
    "md": "phpmd src text phpmd.xml",
    "md-xml": "phpmd src xml phpmd.xml",
    "analyze": "vendor/bin/phan --color --progress-bar"
  },
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "symfony/http-foundation": "^4.1",
    "symfony/validator": "^4.1",
    "phan/phan": "^2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "doctrine/orm": "^2.7",
    "spatie/phpunit-snapshot-assertions": "^2.2",
    "symfony/var-dumper": "^4.4",
    "squizlabs/php_codesniffer": "^3.5",
    "phpmd/phpmd": "^2.8"
  },
  "autoload": {
    "psr-4": {
      "Jad\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jad\\Tests\\": "tests/src",
      "Jad\\Database\\": "tests/Database",
      "Jad\\E2E\\": "tests/e2e"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
